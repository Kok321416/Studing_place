[flake8]
max-line-length = 88
exclude = 
    .venv,
    venv,
    env,
    __pycache__,
    .git,
    migrations,
    node_modules,
    .tox,
    .eggs,
    *.egg,
    build,
    dist
ignore = 
    E203,
    W503,
    E501,
    F401,
    F841,
    F541,
    E402
per-file-ignores =
    migrations/*:E501,F401
    */migrations/*:E501,F401
    settings.py:E501,F401
    */settings.py:E501,F401
